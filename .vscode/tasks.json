{
    "version": "0.1.0",
    "command": "npm",
    "args": [ "run" ],
    "isShellCommand": true,
    "showOutput": "always",
    "suppressTaskName": true,
    "tasks": [{
        "taskName": "build",
        "isBuildCommand": true,
        "args": [ "build-js" ]
    }, {
        "taskName": "test",
        "isTestCommand": true,
        "args": [ "test" ]
    }, {
        "taskName": "coverage",
        "isTestCommand": true,
        "args": [ "coverage" ]
    }, {
        "taskName": "transpile",
        "args": [ "transpile" ]
    }]
}
